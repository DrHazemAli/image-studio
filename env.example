# Azure GenAI Image Studio Environment Variables
# Copy this file to .env and fill in your values

# Azure Configuration (Required)
AZURE_API_BASE_URL=https://your-resource-name.openai.azure.com
AZURE_API_KEY=your-azure-api-key-here

# Application Configuration
NODE_ENV=production
PORT=3000

# Optional: Debug Configuration
DEBUG=false

# Optional: Custom Azure Endpoints (if using custom deployments)
# AZURE_DALLE_ENDPOINT=https://your-resource-name.openai.azure.com/openai/deployments/dalle-3
# AZURE_FLUX_ENDPOINT=https://your-resource-name.openai.azure.com/openai/deployments/FLUX-1.1-pro
